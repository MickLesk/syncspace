    "author": "SyncSpace Contributors",
    "license": "MIT",
    "files": [
        "atoms",
        "molecules",
        "organisms",
        "pages",
        "shared",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/syncspace.git",
        "directory": "frontend_components"
    },
    "peerDependencies": {
        "svelte": "^5.0.0",
        "tailwindcss": "^4.0.0"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/vite-plugin-svelte": "^4.0.4",
        "@tailwindcss/postcss": "^4.0.0",
        "@types/node": "^20.0.0",
        "postcss": "^8.4.0",
        "svelte": "^5.0.0",
        "tailwindcss": "^4.0.0",
        "typescript": "^5.0.0",
        "vite": "^5.0.0"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
