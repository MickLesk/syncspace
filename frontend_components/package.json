{
    "name": "@syncspace/components",
    "version": "1.0.0",
    "description": "SyncSpace Component Library - Production-ready UI components built with Svelte 5, TypeScript, and Tailwind CSS v4",
    "type": "module",
    "main": "./atoms/index.ts",
    "exports": {
        ".": "./atoms/index.ts",
        "./atoms": "./atoms/index.ts",
        "./molecules": "./molecules/index.ts",
